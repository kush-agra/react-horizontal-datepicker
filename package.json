{
  "name": "react-horizontal-datepicker",
  "version": "2.0.1",
  "homepage": "https://github.com/kush-agra/react-horizontal-datepicker",
  "repository": {
    "type": "git",
    "url": "https://github.com/kush-agra/react-horizontal-datepicker.git"
  },
  "keywords": [
    "date-picker",
    "horizontal-calendar",
    "horizontal-datepicker",
    "swipeable",
    "side-scrolling",
    "calendar-strip",
    "web"
  ],
  "private": false,
  "main": "./dist/DatePicker.js",
  "license": "MIT",
  "author": "Kushagra Agrawal",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "date-fns": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "publish:npm": "rm -rf dist && mkdir dist && babel src/components -d dist --copy-files",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-react": "^7.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.13"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
